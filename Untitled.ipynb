    "def MEDIA (lista):\n",
    "    somma = 0\n",
    "    \n",
    "    for i in lista:\n",
    "        somma+= i\n",
    "    media = somma / len(lista)\n",
    "    return media\n",
    "\n",
    "freq = MEDIA(l)\n",
    "print(\"La tua frequenza è : \"  + str (freq)) \n",
    "    \n",
    "\n",
    "\n",
    "def FCMAX (s, a):\n",
    "    if s == \"femmina\":\n",
    "        fm = 226 - a\n",
    "    else:\n",
    "        fm = 220 - a\n",
    "    print(\"La tua frequenza cardiaca massima è: \" + str(fm))\n",
    "    return fm\n",
    "\n",
    "age = text.value\n",
    "gender = buttonGender.value\n",
